<?xml version="1.0" encoding="utf-8"?>
<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!-- Main Colors -->
    <Color x:Key="AppDarkBackgroungColor">#1d1c1c</Color>
    <Color x:Key="PageDarkBackgroungColor">#232222</Color>
    <Color x:Key="AppLightBackgroungColor">#bcc6ea</Color>
    <Color x:Key="PageLightBackgroungColor">#cdd3eb</Color>
    <Color x:Key="BrowserDarkTabBackgroundColor">#1e1e1e</Color>
    <Color x:Key="BrowserLightTabBackgroundColor">#bbc2df</Color>
    <Color x:Key="TextPalleteForegroundColor">#43668e</Color>
    
    <!-- Basic Colors -->
    <Color x:Key="White">#fcfcfc</Color>
    <Color x:Key="Black">#010101</Color>
    <Color x:Key="Transparent">Transparent</Color>

    <Color x:Key="SectionDarkBackgroundColor">#363434</Color>
    <Color x:Key="SectionLightBackgroundColor">#dfe3f4</Color>
    
</ResourceDictionary>
