<?xml version="1.0" encoding="utf-8"?>
<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!-- HighContrast Themes XAML -->

    <SolidColorBrush x:Key="AppBackground" Color="{ThemeResource SystemColorBackgroundColor}"/>
    <SolidColorBrush x:Key="PageBackground" Color="{ThemeResource SystemColorActiveCaptionColor}"/>

    <SolidColorBrush x:Key="TitleMainTexts" Color="{ThemeResource SystemColorActiveCaptionColor}"/>
    <SolidColorBrush x:Key="AppMainTexts" Color="{ThemeResource SystemColorActiveCaptionColor}"/>

    <SolidColorBrush x:Key="SectionPanelBackground" Color="{ThemeResource SystemColorBackgroundColor}"/>

    <!-- Browser Page -->
    <SolidColorBrush x:Key="TabItemBackground" Color="{ThemeResource SystemColorBackgroundColor}"/>

</ResourceDictionary>
