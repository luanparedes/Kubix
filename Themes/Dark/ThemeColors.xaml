<?xml version="1.0" encoding="utf-8"?>
<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!-- Dark Themes XAML -->

    <SolidColorBrush x:Key="AppBackground" Color="{ThemeResource AppDarkBackgroungColor}"/>
    <SolidColorBrush x:Key="PageBackground" Color="{ThemeResource PageDarkBackgroungColor}"/>

    <SolidColorBrush x:Key="TitleMainTexts" Color="{ThemeResource TextPalleteForegroundColor}"/>
    <SolidColorBrush x:Key="AppMainTexts" Color="{ThemeResource AppLightBackgroungColor}"/>

    <SolidColorBrush x:Key="SectionPanelBackground" Color="{ThemeResource SectionDarkBackgroundColor}"/>
    
    <!-- Browser Page -->
    <SolidColorBrush x:Key="TabItemBackground" Color="{ThemeResource BrowserDarkTabBackgroundColor}"/>
    

</ResourceDictionary>
